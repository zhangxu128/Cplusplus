package moxing;
import java.util.*;
import java.net.*;
import java.io.*;

public class Lianjie 
{
	public Lianjie()
	{
		try {
			Socket s=new Socket("127.0.0.1",9999);
		} catch (Exception e) {}
	}
	public void fasong(Object o)
	{
		try {
			Socket s=new Socket("127.0.0.1",9999);		
			ObjectOutputStream oos=new ObjectOutputStream(s.getOutputStream());
			oos.writeObject(o);
		} catch (Exception e) {}
	}
}