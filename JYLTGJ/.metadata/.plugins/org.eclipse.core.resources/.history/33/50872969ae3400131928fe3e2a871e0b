package moshi;
import java.util.*;
import java.net.*;
import java.io.*;
import gonggong.*;

public class Xiancheng extends Thread
{
    Socket s;
	
	public Xiancheng(Socket s)
	{
		this.s=s;
	}	
	public void run()
	{		
		while(true)
		{
			try {
				ObjectInputStream ois=new ObjectInputStream(s.getInputStream());
				Message m=(Message)ois.readObject();
				System.out.println(m.getSender()+" ธ๘ "+m.getGetter()+" หต:"+m.getCon());
				
			} catch (Exception e) {}			
		}		
	}
}